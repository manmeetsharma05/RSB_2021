<script type="text/javascript">
    function dbind() {


        $.ajax({
            type: "post",
            url: '@Url.Action("district", "Home")',
            data: { stateId: $('#StateStatus').val() },
            datatype: "json",
            traditional: true,
            success: function (data) {
                var district = "<select id='DistrictStatus'>";
                district = district + '<option value="">--Select--</option>';

                for (var i = 0; i < data.length; i++) {
                    district = district + '<option value=' + data[i].Value + '>' + data[i].Text + '</option>';
                }
                district = district + '</select>';
                $('#DistrictStatus').html(district);
            }
        });
    }
    //bind district

    //bind tehsil
    function tbind(dist, teh) {


        $.ajax({
            type: "post",
            url: '@Url.Action("Tehsil", "Home")',
            data: { districtId: dist },
            datatype: "json",
            traditional: true,
            success: function (data) {
                var tehsil = "<select id='tname'>";


                for (var i = 0; i < data.length; i++) {
                    if (teh == data[i].Value) {
                        tehsil = tehsil + '<option value=' + data[i].Value + ' selected>' + data[i].Text + '</option>';
                    }
                    else {

                        tehsil = tehsil + '<option value=' + data[i].Value + '>' + data[i].Text + '</option>';
                    }
                }
                tehsil = tehsil + '</select>';
                $('#tname').html(tehsil);
            }
        });
    }
    //bind tehsil with one parameter
    function tehbind(dist) {


        $.ajax({
            type: "post",
            url: '@Url.Action("Tehsil", "Home")',
            data: { districtId: dist },
            datatype: "json",
            traditional: true,
            success: function (data) {

                var tehsil = "<select id='tname'>";
                tehsil = tehsil + '<option value="">--Select--</option>';
                for (var i = 0; i < data.length; i++) {
                    tehsil = tehsil + '<option value=' + data[i].Value + '>' + data[i].Text + '</option>';
                }
                tehsil = tehsil + '</select>';
                $('#tname').html(tehsil);
            }
        });
    }
    //bind force
    function bindf(fname) {
        $.ajax({
            type: "post",
            url: '@Url.Action("forcenamedept", "Home")',
            data: { forcedeptid: fname },
            datatype: "json",
            traditional: true,
            success: function (data) {
                var forcecat = "<select id='ForceCatStatus'>";

                for (var i = 0; i < data.length; i++) {
                    if (data[i].value == fname) {
                        forcecat = forcecat + '<option value=' + data[i].Value + ' selected>' + data[i].Text + '</option>';
                    }
                    else {
                        forcecat = forcecat + '<option value=' + data[i].Value + '>' + data[i].Text + '</option>';
                    }
                }
                forcecat = forcecat + '</select>';
                $('#ForceCatStatus').html(forcecat);
            }
        });
    }



</script>
