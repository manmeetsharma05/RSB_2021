<script type="text/javascript">

    function searchdbind(districtCode, stateCode) {
        if (stateCode == "HA") {
            stateCode = "06";
        }
        else {
            stateCode = stateCode;
        }

        $.ajax({
            type: "post",
            url: '@Url.Action("district", "Home")',
            data: { stateId: stateCode },
            datatype: "json",
            traditional: true,
            success: function (data) {
                var district = "<select id='DistrictStatus'>";


                for (var i = 0; i < data.length; i++) {

                    if (data[i].Value == districtCode) {
                        district = district + '<option value=' + data[i].Value + ' selected>' + data[i].Text + '</option>';
                    }
                    else {
                        district = district + '<option value=' + data[i].Value + '>' + data[i].Text + '</option>';
                    }
                }
                district = district + '</select>';
                $('#DistrictStatus').html(district);
            }
        });
    }
    $(document).ready(function () {

        $.ajax(
                  {

                      dataType: "json",
                      type: "POST", //HTTP POST Method  
                      url: '@Url.Action("RSBGeneralInfrm1", "Home")', // Controller/View   
                      data: "",
                      success: function (response) {

                          if (response.user != null || response.user != "") {
                              $("#btnlogout").show();
                              $("#btnhome").hide();
                              if (response.usertype == "72") {
                                  $("#sc").show();
                                  $("#med").show();
                                  $("#awd").show();
                                  $("#rnk").show();
                                  $("#regcrp").show();
                                  $("#pens").show();
                                  //images and links hide 
                                  $("#sc1").show();
                                  $("#med1").show();
                                  $("#awd1").show();
                                  $("#rnk1").show();
                                  $("#regcrp1").show();
                                  $("#pens1").show();
                                  $("#btnfrz").show();
                                  $("#btnhome").hide();
                                  $("#help").show();
                                  $("#char").show();

                              }
                              else if (response.usertype == "70") {
                                  $("#sc").hide();
                                  $("#med").hide();
                                  $("#awd").hide();
                                  $("#rnk").hide();
                                  $("#regcrp").hide();
                                  $("#pens").hide();
                                  //images and links hide 
                                  $("#sc1").hide();
                                  $("#med1").hide();
                                  $("#awd1").hide();
                                  $("#rnk1").hide();
                                  $("#regcrp1").hide();
                                  $("#pens1").hide();
                                  $("#btnfrz").hide();
                                  $("#help").show();
                                  $("#char").show();
                                  $("#vv").hide();
                                  var state = $("#StateStatus").val();
                                  // $("#DistrictStatus").val(response.dco);
                                  searchdbind(response.dco, state);
                              }
                              else {
                                  $("#sc").show();
                                  $("#med").show();
                                  $("#awd").show();
                                  $("#rnk").show();
                                  $("#regcrp").show();
                                  $("#pens").show();
                                  //images and links hide 
                                  $("#sc1").show();
                                  $("#med1").show();
                                  $("#awd1").show();
                                  $("#rnk1").show();
                                  $("#regcrp1").show();
                                  $("#pens1").show();
                                  $("#btnfrz").show();
                                  $("#btnhome").hide();
                                  $("#help").show();
                                  $("#char").show();
                              }
                          }
                          else {
                              window.location.href = 'Home/Login';
                          }


                      },



                  });
    });
</script>


