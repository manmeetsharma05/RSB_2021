<script type="text/javascript">
    $("#searchdialog").dialog({
        title: 'Alert',
        autoOpen: false,
        modal: true
    });
    function goDoSomethinguid(uid, mobileno) {
        $("#UpdatePaneluidsearch").css('display', 'none');

        var search = mobileno;
        var uidnn = uid;
        $("#serach").val(search);
        $("#value").val(uidnn);


        $.ajax({
            url: "@Url.Action("getuid", "Home")",
            data: {
                uidnn: JSON.parse(JSON.stringify(uidnn)),
                search: search

            },
            async: false,
            type: "post",
            cache: false,

            success: function (returnedData) {
                if (returnedData.Army_No == null) {
                    $("#Army_No").attr('readonly', false);
                    $("#Army_No").css({ "background-color": "#f5f5f5" });
                }
